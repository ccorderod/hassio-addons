{
    "name": "Syncthing",
    "version": "0.14.55",
    "slug": "syncthing",
    "description": "syncthing synchronization service",
    "url": "https://github.com/bestlibre/hassio-addons/tree/master/syncthing",
    "startup": "system",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "i386"
    ],
    "boot": "auto",
    "image": "bestlibre/{arch}-syncthing",
    "options": {},
    "schema": {},
    "ports": {"8384/tcp": 8384, "22000/tcp": null, "21027/udp": 21027},
    "map": ["share:rw", "config:rw", "backup:rw", "addons:rw"],
    "webui": "http://[HOST]:[PORT:8384]"
}
